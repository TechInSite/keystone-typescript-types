{
    "name": "keystone-typescript-types",
    "version": "1.0.4-alpha",
    "description": "A work in progress repo to get the types for KeystoneJS 5 ready to contribute to Definitely Typed.",
    "scripts": {
        "compile-scripts": "tsc -p scripts",
        "format": "prettier --write 'types/**/*.ts'",
        "not-needed": "node scripts/not-needed.js",
        "update-codeowners": "node scripts/update-codeowners.js",
        "test": "node node_modules/types-publisher/bin/tester/test.js --run-from-definitely-typed --all",
        "lint": "dtslint types"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/TechInSite/keystone-typescript-types.git"
    },
    "author": "Kevin Brown",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/TechInSite/keystone-typescript-types/issues"
    },
    "homepage": "https://github.com/TechInSite/keystone-typescript-types#readme",
    "devDependencies": {
        "@types/express": "^4.17.2",
        "@types/react": "^16.9.11",
        "dtslint": "latest",
        "prettier": "^1.17.1",
        "types-publisher": "github:Microsoft/types-publisher#production"
    }
}
